{
  "include": [
    "app/javascript",
    "app/assets/",
    "config/assets/",
    "./.eslintrc.js"
  ],
  "compilerOptions": {
    "outDir": "./dist/",
    "strict": true,
    "module": "es2020",
    "target": "es2017",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  }
}
