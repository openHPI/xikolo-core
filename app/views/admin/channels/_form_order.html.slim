- content_for :head_assets
  = javascript_include_tag 'app/javascript/admin/admin', type: 'module', async: true

= simple_form_for(:channels) do |f|
  = render Form::DynamicSortableList.new(channels.channels_order_select, name: 'positions[]')
  = f.submit t(:'admin.channels.form.label.submit'), class: 'btn btn-primary'
  = link_to t(:'global.cancel'), :back, class: 'btn btn-default ml10'
