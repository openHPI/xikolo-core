<row>
  <columns>
    <% if @payload.test? %>
      <callout class="warning">
        <row>
          <columns>
            <%= t '.test_notice' %>
          </columns>
        </row>
      </callout>
    <% end %>

    <div dir="auto">
      <%= raw NewsService::MarkdownService.render_html @payload.content %>
    </div>
  </columns>
</row>
